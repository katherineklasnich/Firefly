<%= simple_form_for [@camera] do |f| %>
  <%= f.input :name %>
  <%= f.input :brand %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.date_select :start_date %>
  <%= f.date_select :end_date %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
